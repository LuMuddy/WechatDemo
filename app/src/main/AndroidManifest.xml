<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="muddy.top.wechatdemo">

    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:resizeableActivity="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!-- 启动页 -->
        <activity android:name=".Activity.Welcome.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- 语言设置 -->
        <activity android:name=".Activity.Welcome.LanguageSettingActivity" />
        <!-- 登陆 -->
        <activity android:name=".Activity.LoginRegistry.LoginActivity" />
        <!-- 注册 -->
        <activity android:name=".Activity.LoginRegistry.RegistryActivity" />
        <!-- 主页 -->
        <activity android:name=".Activity.Home.MainActivity" />
        <activity android:name=".Activity.ChooseCountryActivity" />
        <activity android:name=".Activity.LoginRegistry.DisplayActivity" />
        <activity android:name=".Activity.WebIntentactivity" />
        <activity android:name=".Activity.LoginRegistry.MoblieLoginActivity"></activity>
    </application>

</manifest>